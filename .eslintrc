
{
    "extends": "airbnb/base",
    "rules": {
        "indent": [2, 4],
        "func-names": 0,
        "no-param-reassign": 0,
        "arrow-body-style": 0,
        "prefer-rest-params": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0
    },
    "globals": {
        "log": true
    }
}
